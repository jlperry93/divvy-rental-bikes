{
  "name": "divvy-bike-rental",
  "description": "A service for the Divvy bike rental company",
  "version": "0.1.0",
  "author": "Jordan Perry",
  "contributors": [],
  "engines": {
    "node": "~12.13.0"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "csv-parser": "^2.3.2",
    "express": "^4.17.1",
    "fs": "0.0.1-security",
    "http": "0.0.0",
    "https": "^1.0.0"
  },
  "devDependencies": {
    "@types/csv-parse": "^1.2.2",
    "@types/express": "^4.17.2",
    "@types/jest": "^24.0.23",
    "@types/node": "^12.12.17",
    "jest": "^24.9.0",
    "supertest": "^4.0.2",
    "tslint": "^5.20.1",
    "typescript": "^3.7.3"
  },
  "main": "dist/server.js",
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "src"
    ],
    "exec": "npm start",
    "ext": "ts"
  },
  "jest": {
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ]
  },
  "scripts": {
    "prebuild": "tslint -c tslint.json -p tsconfig.json --fix",
    "build": "tsc",
    "prestart": "npm run build",
    "start": "node .",
    "test": "jest "
  }
}